{
    "name": "Rclone HerokuShell",
    "description": "Bot to interface Heroku shell through Telegram with Rclone",
    "keywords": [
        "telegram",
        "shell",
        "rclone",
        "heroku"
    ],
    "repository": "https://github.com/Pavithran-R/RcloneHeroku-Shell.git",
    "env": {
      "BOT_TOKEN": {
        "description": "The bot token provided by @BotFather",
        "required": true
      },
      "OWNER_ID": {
        "description": "Your Telegram user ID, get it from @userinfobot",
        "value": "",
        "required": true
      },
      "RCLONE_CONFIG": {
        "description": "Your Rclone config url",
        "required": false
      },
      "ACCOUNTS_FOLDER_LINK": {
        "description": "enter permanent download link(maybe from index) of your accounts.zip",
        "required": false
      }
     },
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ]
  }
